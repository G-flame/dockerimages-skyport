{
    "Image": "quay.io/skyport/java:21",
    "Cmd": "docker pull quay.io/skyport/java:21",
    "Env": [
        "START=java -jar server.jar"
    ],
    "Scripts": {
        "Install": [
            {
                "Uri": "https://github.com/AllayMC/Allay/releases/download/0.1.0/allay-server-0.1.0-28c0139-shaded.jar",
                "Path": "server.jar"
            },
            {
                "Uri": "https://raw.githubusercontent.com/G-flame/dockerimages-skyport/refs/heads/main/README.md",
                "Path": "README.md"
            }
        ]
    },
    "Name": "MC:bedrock",
    "Description": "minecraft bedrock edition for skyport using the allay loader",
    "Author": "thegreenflame1507@gmail.com",
    "AuthorName": "G_flame",
    "Meta": {
        "Version": "0.01"
    }
}
